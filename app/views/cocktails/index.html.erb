<div>
  <%= "List of all our cocktails" %>
</div>
<div>
  <%= link_to "Create a new cocktail", new_cocktail_path %>
</div>



<% @cocktails.each do |cocktail| %>
  <h3><%= cocktail.name %></h3>
  <%= link_to cocktail.name, cocktail_path(cocktail) %>
  <hr>
<% end %>

<div>
  <%= "Thank you for coming" %>
</div>
